<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uuzaz.dailynote">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_dailynote"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.uuzaz.dailynote.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.uuzaz.dailynote.MainActivity" />
        <activity android:name="com.example.uuzaz.dailynote.Planning"/>
        <activity android:name="com.example.uuzaz.dailynote.Look_Back" />
        <activity android:name="com.example.uuzaz.dailynote.PastSchedule" />
        <activity android:name="com.example.uuzaz.dailynote.StartingActivity" ></activity>
        // 데이터베이스 진짜 쉽네
    </application>

</manifest>